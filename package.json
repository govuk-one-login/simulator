{
  "name": "simulator",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/server.js",
    "check:lint": "eslint . --ext .ts,.js",
    "check:pretty": "prettier --check \"*/**/*.{ts,json}\"",
    "check": "npm run check:lint && npm run check:pretty",
    "fix:lint": "eslint . --ext .ts,.js --fix",
    "fix:pretty": "prettier --write \"*/**/*.{ts,json}\"",
    "fix": "npm run fix:lint && npm run fix:pretty",
    "prepare": "husky",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": true
  },
  "dependencies": {
    "body-parser": "^1.20.3",
    "express": "^4.21.0",
    "jose": "^5.9.3",
    "pino": "^9.4.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.13",
    "@types/node": "^22.7.4",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.7.0",
    "@typescript-eslint/parser": "^8.8.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.6.3",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.2"
  }
}
